  "scripts": {
    "build": "tsc && vite build",
    "build-storybook": "build-storybook",
    "check": "npm-check --skip-unused --update-all",
    "predev": "npm run node-shield",
    "dev": "vite",
    "lighthouse": "lighthouse http://localhost:8000/",
    "prelint": "prettier --write **/src/**/*.{js,jsx,ts,tsx,css,md,json}  --config ./.prettierrc",
    "lint": "eslint --config ./.eslintrc.json  --color --fix --ext .js,.jsx,.ts,.tsx src",
    "postlint": "eslint  --config ./.eslintrc.json  --color --max-warnings=0 --ext .js,.jsx,.ts,.tsx src",
    "node-shield": "nvm-shield --version=16 --compare=major && echo 'Node version is CORRECT' || (echo 'Node version is INCORRECT please run \"nvm use\"' && exit 1) ",
    "restart": "echo 'Make sure node version 16>'&& rm -rf node_modules && rm -f package-lock.json  && npm i && npm run dev",
    "storybook": "start-storybook -p 6006",
    "pretest": "npm run node-shield",
    "test": "vitest  --reporter verbose --watch --dir ./src/test",
    "test:ui": "vitest --ui",
    "test:w": "vitest",
    "typedoc": "typedoc  --tsconfig 'tsconfig.json' ",
    "preview": "vite preview",
    "why": "npx why",
    "write": "onchange './src/**/*.ts' './src/**/*.tsx' -- npm run lint"
  },
